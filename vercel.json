{
    "rewrites": [
        { "source": "/(.*)", "destination": "/" }
    ],
    "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": ""
        },
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors https://framer.com https://*.framer.com https://*.framer.app https://bu-ul.com https://*.bu-ul.com"
        }
      ]
    }
  ]
}
